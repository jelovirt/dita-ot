<!--Declare the domains-->
<!ENTITY included-domains "&ui-d-att; &hi-d-att; &pr-d-att; &sw-d-att;
                           &ut-d-att; &api-d-att; &javaapi-d-att;">

<!--Topic nesting uses defaults-->

<!ENTITY % topicDefns PUBLIC "-//OASIS//ENTITIES DITA Topic Definitions//EN"
  "../../../dtd/topicDefn.ent">
