        <section conref="../packagedef.dita#cxxVariableDeclarationFileLine-reference/cxxVariableDeclarationFileLine-attList-section"/>
        <section conref="../packagedef.dita#cxxVariableDeclarationFileLine-reference/cxxVariableDeclarationFileLine-classValue-section"/>
    </refbody>
</reference>