        <section conref="../packagedef.dita#cxxFunctionStorageClassSpecifierStatic-reference/cxxFunctionStorageClassSpecifierStatic-contentModel-section"/>
        <section conref="../packagedef.dita#cxxFunctionStorageClassSpecifierStatic-reference/cxxFunctionStorageClassSpecifierStatic-attList-section"/>
        <section conref="../packagedef.dita#cxxFunctionStorageClassSpecifierStatic-reference/cxxFunctionStorageClassSpecifierStatic-classValue-section"/>
    </refbody>
